algoritmo
	declare ab, cd, ef numerico
	escreva "Valor para 'ab'"
   leia ab
   
   se ab < 10
   entao 
   	inicio
   		ab <-- 100
   		escreva "agora ab recebeu 100"
   	fim
   senao 
   	inicio
   		escreva "Valor para 'cd'"
   		leia cd
   	fim 
   
   para ef <-- 1 ate (ab+cd) faca
   	inicio
		escreva "A"
   	fim
   	escreva "ef", ef
fim algoritmo

